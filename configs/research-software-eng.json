{
    "_id": "research-software",
    "startUrl":
    [
        "https://researchsoftware.org/blog.html"
    ],
    "selectors":
    [
        {
            "id": "article",
            "parentSelectors":
            [
                "_root"
            ],
            "type": "SelectorLink",
            "selector": "a.post-link",
            "multiple": true,
            "delay": 0
        },
        {
            "id": "title",
            "parentSelectors":
            [
                "article"
            ],
            "type": "SelectorText",
            "selector": "h1",
            "multiple": false,
            "delay": 0,
            "regex": ""
        },
        {
            "id": "published",
            "parentSelectors":
            [
                "article"
            ],
            "type": "SelectorText",
            "selector": "time",
            "multiple": false,
            "delay": 0,
            "regex": ""
        },
        {
            "id": "content",
            "parentSelectors":
            [
                "article"
            ],
            "type": "SelectorText",
            "selector": "div[itemprop='articleBody']",
            "multiple": false,
            "delay": 0,
            "regex": ""
        },
        {
            "id": "author",
            "parentSelectors":
            [
                "article"
            ],
            "type": "SelectorText",
            "selector": "ul:nth-of-type(1)",
            "multiple": false,
            "delay": 0,
            "regex": ""
        }
    ]
}